version: "3.7"

services:
  maria:
    image: mariadb
    container_name: maria
    ports:
      -  3306:3306
    environment:
       MYSQL_ROOT_PASSWORD: 1234
    volumes:
       - /home:/var/lib/mysql
  

  boot_application:
    image: wjsguswls71/test-spring-boot
    ports:
      - 8080:8080
    depends_on:
      - maria 
 
